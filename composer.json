{
    "name": "jdev/okkauth",
    "description": "This is a test package for authorisation via OKK",
    "license": "FTL",
    "version": "1.0.2",
    "authors": [
        {
            "name": "Viktor",
            "email": "dev@temposys.name",
            "homepage": "http://jdev.com.ua"
        }
    ],
    "homepage": "https://github.com/MrGeseR/okkAuth",
    "keywords": ["Laravel", "OkkAuth"],
    "require": {
        "illuminate/support": "~5",
        "php": "^7.1.3",
        "ext-json": "*",
        "laravel/socialite": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "jDev\\OkkAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jDev\\OkkAuth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jDev\\OkkAuth\\OkkAuthServiceProvider"
            ],
            "aliases": {
                "OkkAuth": "jDev\\OkkAuth\\Facades\\OkkAuth"
            }
        }
    }
}
